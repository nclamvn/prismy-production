{
  "name": "prismy",
  "version": "1.0.1-fix-nav",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:mutation": "stryker run",
    "test:mutation-info": "stryker run --logLevel info",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:e2e": "playwright test --config playwright-e2e.config.ts",
    "test:e2e:ui": "playwright test --ui --config playwright-e2e.config.ts",
    "test:e2e:large-files": "playwright test --config playwright-e2e.config.ts tests/e2e/large-file-processing.spec.ts",
    "test:e2e:batch": "playwright test --config playwright-e2e.config.ts tests/e2e/large-file-processing.spec.ts --grep \"Batch Processing\"",
    "test:e2e:chaos": "playwright test --config playwright-e2e.config.ts tests/e2e/chaos-testing.spec.ts",
    "test:e2e:load": "playwright test --config playwright-e2e.config.ts tests/e2e/load-testing.spec.ts",
    "test:e2e:performance": "playwright test --config playwright-e2e.config.ts --project performance",
    "test:e2e:headed": "playwright test --config playwright-e2e.config.ts --headed",
    "test:e2e:debug": "playwright test --config playwright-e2e.config.ts --debug",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:edge-functions": "tsx scripts/performance-test-edge-functions.ts",
    "test:edge-functions:moderate": "tsx scripts/performance-test-edge-functions.ts moderate",
    "test:edge-functions:heavy": "tsx scripts/performance-test-edge-functions.ts heavy",
    "test:edge-functions:stress": "tsx scripts/performance-test-edge-functions.ts stress",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "size-limit": "size-limit",
    "lighthouse:ci": "echo 'Lighthouse CI placeholder'",
    "quality:dashboard": "node scripts/quality-dashboard.js",
    "quality:dashboard:json": "node scripts/quality-dashboard.js --format=json",
    "quality:dashboard:md": "node scripts/quality-dashboard.js --format=markdown > reports/quality-dashboard.md",
    "quality:full": "npm run lint && npm run type-check && npm run test:coverage && npm run quality:dashboard",
    "quality:security": "npm audit --audit-level moderate && chmod +x security/run-zap-baseline.sh && ./security/run-zap-baseline.sh",
    "quality:pre-commit": "npm run lint && npm run type-check && npm run test:unit",
    "quality:pre-push": "npm run quality:full && npm run test:mutation",
    "ui:audit": "tsx tools/ui-audit.ts",
    "ui:inventory": "npm run ui:audit",
    "clean": "rm -rf .next out dist coverage",
    "token:export": "tsx scripts/token-export.ts",
    "token:export:css": "tsx scripts/token-export.ts --format=css",
    "token:export:tailwind": "tsx scripts/token-export.ts --format=tailwind",
    "token:export:storybook": "tsx scripts/token-export.ts --format=storybook",
    "token:export:components": "tsx scripts/token-export.ts --format=components",
    "token:export:js": "tsx scripts/token-export.ts --format=js",
    "worker:ocr": "tsx workers/ocr-worker.ts",
    "worker:lang-detect": "tsx workers/lang-detect-worker.ts",
    "worker:translate": "tsx workers/translate-worker.ts",
    "worker:rebuild": "tsx workers/rebuild-worker.ts",
    "worker:finalize": "tsx workers/finalize-worker.ts",
    "workers:all": "concurrently \"npm run worker:ocr\" \"npm run worker:lang-detect\" \"npm run worker:translate\" \"npm run worker:rebuild\" \"npm run worker:finalize\"",
    "ws:server": "tsx lib/websocket/ws-server.ts",
    "ws:dev": "concurrently \"npm run dev\" \"npm run ws:server\"",
    "prepare": "husky",
    "release": "semantic-release",
    "coverage:badge": "npx coverage-badge-creator",
    "build-prod": "next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual:vietnamese": "playwright test --grep vietnamese",
    "test:visual:percy": "percy exec -- playwright test tests/visual/multi-locale.spec.ts",
    "test:visual:mobile": "playwright test tests/visual/mobile-specific.spec.ts",
    "test:visual:rtl": "playwright test tests/visual/rtl-layout.spec.ts",
    "test:visual:storybook": "percy storybook --widths=375,768,1024,1440,1920",
    "test:visual:all": "npm run test:visual:percy && npm run test:visual:storybook",
    "quality:ui-report": "node scripts/ui-quality-gates.js",
    "quality:gates": "npm run quality:ui-report",
    "quality:a11y": "tsx scripts/quality-automation.ts --focus=a11y",
    "quality:i18n": "tsx scripts/quality-automation.ts --focus=i18n",
    "quality:auto": "tsx scripts/quality-automation.ts",
    "quality:auto:json": "tsx scripts/quality-automation.ts --format=json",
    "quality:auto:ci": "tsx scripts/quality-automation.ts --format=json --output=reports/ci-quality.json",
    "deploy:production": "npm run quality:auto:ci && npm run quality:gates && vercel --prod",
    "deploy:preview": "vercel",
    "demo:setup": "tsx scripts/demo-setup.ts",
    "demo:start": "cp .env.demo .env.local && npm run dev",
    "demo:seed": "tsx scripts/demo-seed.ts",
    "demo:reset": "tsx scripts/demo-reset.ts",
    "demo:deploy": "vercel --prod --env NEXT_PUBLIC_DEMO_MODE=true"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
